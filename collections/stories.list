<div class="stories -exp" data-collection-id="{collection.id}">
    <div class="-wrap">
        <h1>{collection.title}</h1>
    </div>
    <div class="grid">
        {.repeated section items}
            <div class="grid__item">
                <a class="grid__link" href="{fullUrl}" data-item-id="{id}">
                    <img class="grid__image image js-lazy-image" data-img-src="{assetUrl}" data-variants="{systemDataVariants}" data-original-size="{originalSize}" />
                    <div class="grid__title">
                        <p data-content-field="title">{title}</p>
                    </div>
                </a>
            </div>
        {.end}
    </div>
</div>
