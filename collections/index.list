{.var @homepage collection.homepage}
{.section collection}
    {.repeated section collections}
        {.if items}
            {.repeated section items}
                {.equal? customContent.customType "customCover"}
                    {@|apply cover.block}
                {.end}
            {.end}
        {.end}
        {.if mainContent}
            {mainContent}
        {.end}
    {.end}
{.end}
{.if @homepage}
    <squarespace:block-field id="deem--footer" columns="12" />
{.end}
