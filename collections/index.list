{.section collection}
    {.repeated section collections}
        {.if items}
            {.section items.0}
                {.equal? customContent.customType "customCover"}
                    {@|apply covers.block}
                {.end}
            {.end}
        {.end}
        {.if mainContent}
            {mainContent}
        {.end}
    {.end}
{.end}
